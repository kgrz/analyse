table.table-hover.table.table-condensed
	thead
		tr
			th Assets
			th Size
			th Chunks
			th Names
			th Flags
	tbody
		each asset in stats.assets
			tr
				td
					= asset.name
				td= require("../../formatSize")(asset.size)
				td
					each chunk in asset.chunks
						a(href=`#chunk/${chunk}`)= chunk
						= " "
				td
					each name in asset.chunkNames
						code= name
						= " "
				td
					if module.emitted
						span.label.label-success emitted
						= " "
